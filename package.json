    "color": "^3.0.0",
    "hex-rgb": "^3.0.0",
    "rgb-to-hex": "0.0.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2",
    "ts-jest": "^23.10.0-beta.6",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
